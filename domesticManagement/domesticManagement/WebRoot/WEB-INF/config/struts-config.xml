<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC  
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"  
          "http://struts.apache.org/dtds/struts-config_1_3.dtd"> 

<struts-config>
	<form-beans>
		<form-bean name="domesticForm" type="com.ninebrains.form.DomesticForm"/>
	</form-beans>

	<action-mappings>
		<action path="/login" type="com.ninebrains.action.DomesticAction" name="domesticForm" scope="request">
			<forward name="frame" path="/jsp/frame.jsp"></forward>
		</action>
		<action path="/add" type="com.ninebrains.action.DomesticAction" name="domesticForm" scope="request">
			<forward name="test" path="/jsp/test.jsp"></forward>
		</action>
		<action path="/list" type="com.ninebrains.action.DomesticAction" name="domesticForm" scope="request">
			<forward name="list" path="/jsp/list.jsp"></forward>
		</action>
		<action path="/edit" type="com.ninebrains.action.DomesticAction" name="domesticForm" scope="request">
			<forward name="edit" path="/jsp/edit.jsp"></forward>
		</action>
		<action path="/update" type="com.ninebrains.action.DomesticAction" name="domesticForm" scope="request">
			<forward name="test" path="/jsp/test.jsp"></forward>
		</action>
		<action path="/delete" type="com.ninebrains.action.DomesticAction" name="domesticForm" scope="request">
			<forward name="test" path="/jsp/test.jsp"></forward>
		</action>
		<action path="/query" type="com.ninebrains.action.DomesticAction" name="domesticForm" scope="request">
			<forward name="list" path="/jsp/list.jsp"></forward>
		</action>
	</action-mappings>







</struts-config>